{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["expo"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react-native"
  },
  "include": ["App.tsx", "app.config.ts", "src"]
}
